<!DOCTYPE html>
<html>
	<head>
		<meta charset="UTF-8">
		<meta name="viewport" content="width=device-width, initial-scale=1.0">
        <meta content="Responsive bootstrap 4 admin template" name="description"/>
        <meta content="Coderthemes" name="author" />
        <meta http-equiv="X-UA-Compatible" content="IE=edge" />
		<title>
			{% block title %}{% endblock %}
		</title>
		<!-- Custom CSS -->
		<link href="{{ asset('css/styles.css') }}" rel="stylesheet" type="text/css">
		<!-- App CSS -->
		<link href="{{ asset('libs/bootstrap4/bootstrap.min.css') }}" rel="stylesheet" type="text/css"/>
		<link href="{{ asset('libs/bootstrap-tagsinput/bootstrap-tagsinput.css') }}" rel="stylesheet" type="text/css"/>
		<link href="{{ asset('libs/sweetalert2/sweetalert2.min.css') }}" rel="stylesheet" type="text/css"/>
		<link href="{{ asset('libs/sweetalert2/sweetalert2.min.css') }}" rel="stylesheet" type="text/css"/>
		<link href="{{ asset('libs/toastr/toastr.min.css') }}" rel="stylesheet" type="text/css"/>
		<link href="//cdnjs.cloudflare.com/ajax/libs/font-awesome/3.2.1/css/font-awesome.min.css" rel="stylesheet" />
	
		<!-- JS Fontawesone -->
		<script src="https://kit.fontawesome.com/eb9a666b3e.js" crossorigin="anonymous"></script>
		<!-- Bloque CSS personalizados-->
		{% block stylesheets %}{% endblock %}
	</head>
	<body>

		<!-- Menú de navegación -->
		{% block menu %}{% endblock %}
		
		<!-- Bloque buscador -->
		{% block search %}{% endblock %}

		<!-- Bloque contenido dinámico -->
		{% block content %}{% endblock %}


		<!-- Bloque pie de página -->
		{% block footer %}{% endblock %}

		<!-- App JS -->
		<script src="{{ asset('libs/jquery/jquery-3.6.0.min.js') }}"></script>
		<script src="{{ asset('libs/bootstrap4/bootstrap.bundle.min.js') }}"></script>
		<script src="{{ asset('libs/bootstrap-tagsinput/bootstrap-tagsinput.min.js') }}"></script>
		<script src="{{ asset('libs/sweetalert2/sweetalert2.min.js') }}"></script>
		<script src="{{ asset('libs/jquery-validate/jquery.validate.min.js') }}"></script>
		<script src="{{ asset('libs/toastr/toastr.min.js') }}"></script>

		<script src="{{ asset('js/app.js') }}"></script>
		<!--Bloque JS personalizados-->
		{% block javascripts %}{% endblock %}
	</body>
</html>
